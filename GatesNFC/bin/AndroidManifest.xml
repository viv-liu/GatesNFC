<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gatesnfc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.gatesnfc.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.gatesnfc.existing.ExistingActivity"
            android:label="@string/app_name" >
            
            <!-- Handle a collectible card NDEF record -->
            <intent-filter>
      			<action android:name="android.nfc.action.NDEF_DISCOVERED"/>
      			<!-- mimeType is a fake data type that CardActivity is designed to handle  -->
      			<data android:mimeType="application/vnd.example.gatesnfc.existing"/>
      			<category android:name="android.intent.category.DEFAULT"/>
    		</intent-filter>
         </activity>
        <activity android:name="com.example.gatesnfc.New.NewActivity"></activity>
        
    </application>

</manifest>
